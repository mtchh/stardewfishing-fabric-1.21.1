{
  "schemaVersion": 1,
  "id": "stardew_fishing",
  "version": "${version}",
  "name": "stardewfishing-fabric",
  "description": "Making fishing fun with a minigame directly inspired by Stardew Valley(fabric version of Stardew Fishing)!",
  "authors": [
    "kltyton"
  ],
  "contact": {
    "website": "https://github.com/kltyton/stardewfishing-fabric",
    "repo": "https://github.com/kltyton/stardewfishing-fabric"
  },
  "license": "MIT",
  "icon": "assets/stardew_fishing/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.kltyton.stardewfishingFabric.client.ClientEvents"
    ],
    "main": [
      "com.kltyton.stardewfishingFabric.StardewfishingFabric"
    ]
  },
  "mixins": [
    "stardewfishing-fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}"
  },
  "accessWidener" : "stardewfishing-fabric.accesswidener"
}
